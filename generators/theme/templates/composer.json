{
  "name": "<%= prefix %>/<%= shortname %>-theme",
  "description": "<%= shortname %> Theme",
  "version": "v0.0.1",
  "type": "shopware-platform-plugin",
  "license": "proprietary",
  "authors": [{
    "name": "<%= company %>"
  }],
  "autoload": {
    "psr-4": {
      "<%= name %>\\": "src/"
    }
  },
  "extra": {
    "shopware-plugin-class": "<%= name %>\\<%= name %>",
    "label": {
      "de-DE": "<%= description %>",
      "en-GB": "<%= description %>"
    },
    "copyright": "<%= company %>",
    "description": {
      "de-DE": "",
      "en-GB": ""
    },
    "manufacturerLink": {
      "de-DE": "<%= manufacturerLink %>",
      "en-GB": "<%= manufacturerLink %>"
    },
    "supportLink": {
      "de-DE": "<%= supportLink %>",
      "en-GB": "<%= supportLink %>"
    }
  },
  "require": {
    "shopware/core": "~6.4",
    "shopware/storefront": "~6.4"
  }
}
