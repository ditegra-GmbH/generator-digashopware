{
  "name": "<%= prefix %>/<%= shortname %>-plugin",
  "description": "<%= shortname %> Plugin",
  "version": "v0.0.1",
  "type": "shopware-platform-plugin",
  "license": "proprietary",
  "authors": [{
    "name": "<%= company %>"
  }],
  "autoload": {
    "psr-4": {
      "<%= name %>\\": "src/"
    }
  },
  "extra": {
    "shopware-plugin-class": "<%= name %>\\<%= name %>",
    "label": {
      "de-DE": "<%= description %>",
      "en-GB": "<%= description %>"
    },
    "copyright": "<%= company %>",
    "description": {
      "de-DE": "<%= description %>",
      "en-GB": "<%= description %>"
    },
    "manufacturerLink": {
      "de-DE": "<%= manufacturerLink %>",
      "en-GB": "<%= manufacturerLink %>"
    },
    "supportLink": {
      "de-DE": "<%= supportLink %>",
      "en-GB": "<%= supportLink %>"
    }
  },
  "require": {
    "php": ">= 7.4",
    "shopware/core": "~6.4",
    "shopware/storefront": "~6.4"
  },
  "require-dev": {
      "phpunit/phpunit": "^9.5",
      "phpstan/phpstan": "^1.8",
      "friendsofphp/php-cs-fixer": "*",
      "shopware/core": "*",
      "shopware/storefront": "*"
  }
}
